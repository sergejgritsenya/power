  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "webpack": "TS_NODE_POJECT=\"tsconfig.json\" webpack --config webpack.config.ts",
    "docker:build": "docker build -t power .",
    "start": "node -r dotenv/config ./dist/api/entry.js",
    "start:dev": "ts-node-dev -r dotenv/config --no-notify --respawn --transpileOnly ./src/entry.ts",
    "wds": "TS_NODE_POJECT=\"tsconfig.json\" webpack-dev-server --config webpack.config.ts",
    "prisma:save": "./node_modules/.bin/prisma2 migrate save --experimental",
    "prisma:up": "./node_modules/.bin/prisma2 migrate up --experimental",
    "generate": "./node_modules/.bin/prisma2 generate",
    "seed": "ts-node-dev -r dotenv/config migration/seed"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.0",
    "@prisma/client": "^2.0.0-preview022",
    "aws-sdk": "^2.635.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "busboy": "^0.3.1",
    "chyk": "^0.9.1",
    "clsx": "^1.1.0",
    "dotenv": "^8.2.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^8.0.8",
    "mime-types": "^2.1.26",
    "mobx": "^5.15.4",
    "mobx-keystone": "^0.42.1",
    "mobx-react-lite": "^1.5.2",
    "notistack": "^0.9.8",
    "prisma2": "^2.0.0-preview022",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/busboy": "^0.2.3",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/koa": "^2.11.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.0",
    "@types/mime-types": "^2.1.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack": "^4.41.6",
    "@types/webpack-assets-manifest": "^3.0.0",
    "@types/webpack-dev-server": "^3.10.0",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "ts-loader": "^6.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslib": "^1.11.0",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
