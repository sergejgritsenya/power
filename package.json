{
  "name": "power",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:sergejgritsenya/power.git",
  "author": "<sergejgritsenya@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "webpack": "TS_NODE_POJECT=\"tsconfig.json\" webpack --config webpack.config.ts",
    "start": "node -r dotenv/config ./dist/entry.js",
    "start:admin": "node -r dotenv/config ./dist/admin/entry.js",
    "start:api": "node -r dotenv/config ./dist/api/entry.js",
    "start:web": "node -r dotenv/config ./dist/web/entry.js",
    "start:dev": "ts-node-dev -r dotenv/config --no-notify --respawn --transpileOnly ./src/entry.ts",
    "wds": "TS_NODE_POJECT=\"tsconfig.json\" webpack-dev-server --config webpack.config.ts",
    "prisma:save": "./node_modules/.bin/prisma migrate save --experimental",
    "prisma:up": "./node_modules/.bin/prisma migrate up --experimental",
    "generate": "./node_modules/.bin/prisma generate",
    "seed": "ts-node-dev -r dotenv/config migration/seed",
    "docker:build:admin": "docker build -t power-admin -f admin.Dockerfile .",
    "docker:build:api": "docker build -t power-api -f api.Dockerfile .",
    "docker:build:web": "docker build -t power-web -f web.Dockerfile .",
    "docker:start": "docker-compose up -d"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.10",
    "@prisma/cli": "^2.0.0-beta.3",
    "@prisma/client": "^2.0.0-beta.3",
    "aws-sdk": "^2.661.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "busboy": "^0.3.1",
    "chyk": "^0.9.1",
    "clsx": "^1.1.0",
    "dotenv": "^8.2.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "mime-types": "^2.1.26",
    "mobx": "^5.15.4",
    "mobx-keystone": "^0.43.1",
    "mobx-react-lite": "^2.0.6",
    "notistack": "^0.9.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/busboy": "^0.2.3",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.0",
    "@types/koa-static": "^4.0.1",
    "@types/mime-types": "^2.1.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.4",
    "@types/webpack": "^4.41.12",
    "@types/webpack-assets-manifest": "^3.0.0",
    "@types/webpack-dev-server": "^3.10.1",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "ts-loader": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
